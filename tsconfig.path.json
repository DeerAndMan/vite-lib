    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@api/*": [
        "src/api/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@pages/*": [
